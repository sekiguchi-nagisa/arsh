#!/usr/bin/env ydsh

source $SCRIPT_DIR/../module/completion

eval $YDSH_BIN --help | grep '    -' | cut -d ' ' -f 5 | cut -d '[' -f 1 |
while(read -f $'\n') {
    var k = $REPLY.startsWith('--') ? "--long" : "--short"
    var o = $REPLY.startsWith('--') ? $REPLY.from(2) : $REPLY.from(1)
    compdef --cmd ydsh $k $o
}
true
