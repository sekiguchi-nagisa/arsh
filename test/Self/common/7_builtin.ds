## test cd
var a = "$(pwd)"
cd ./
assert($a == "$(pwd)")
assert($a == "`pwd`")

cd ~
import-env HOME
assert($HOME == "$(pwd)")
assert($HOME == "`pwd`")

cd ../../
cd 
assert($HOME == "$(pwd)")
assert($HOME == "`pwd`")

cd /
$a = "$(pwd)"
cd
cd ../
cd /
assert($a == "$(pwd)")
assert($a == "`pwd`")

## test exit
exit
