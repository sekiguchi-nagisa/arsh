#$test($status = 0)

var a = $false
try {
	throw new Error("hello")
} catch($e : Error) {
    assert $? == 0
	$a = $true
	assert($e.message() == 'hello')
}
assert($a)

try {
	throw new OutOfRangeError("a")
} catch($e : Error) {
	assert($e is OutOfRangeError)
	assert($e.message() == "a")
}

try {
    $true ? throw 23 : 54
} catch($e : Error) {
    assert($false)
} catch($e : String) {
    assert($false)
} catch($e) {
    assert($e is Int)
    assert($e as Int == 23)
}