#$test($status = 0, $ignored = 'container|#')

# kill

var e = 34 as Any
try {
    $SIGHUP.kill(1);
    assert $false;
} catch $a {
    assert $? == 0
    $e = $a
}

if $e is SystemError {
    assert $e.message() == if $OSTYPE !~ $/^cygwin/i { "failed to send HUP to pid(1), caused by `Operation not permitted'"; }
                                                else { "failed to send HUP to pid(1), caused by `No such process'"; }
} else {
    assert $false
}
