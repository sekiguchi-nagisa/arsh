
# for -d option (description)
## type signature
assert complete -d -- '$PID.'
assert $COMPREPLY.size() == 4
var args = @($COMPREPLY)
assert $args.size() == 4
assert $args[0] == 'abs@(): Int for Int'
assert $args[1] == 'compare@(target: Int): Int for Int'
assert $args[2] == 'equals@(target: Int): Bool for Int'
assert $args[3] == 'toFloat@(): Float for Int'

assert complete -d -- '$COMPREPLY.a'
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'add@(can: String, desc: String?, space: Int?): Candidates for Candidates'
assert $args[1] == 'addAll@(other: Candidates): Candidates for Candidates'

assert complete -d -- '$OSTY'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'OSTYPE@: String'

assert complete -d -- '$reply.s'
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'size@(): Int for [String : String]'
assert $args[1] == 'swap@(key: String, value: String): String for [String : String]'

assert complete -d -- '
    type AAA{}; 
    function size(): Int for AAA{ return 23; }; 
    function sum(a: Int, b: Int): Int  for AAA{ return $a+$b; }
    new AAA().s'
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'size@(): Int for AAA'
assert $args[1] == 'sum@(a: Int, b: Int): Int for AAA'

## command description
assert complete -d -- 'shct'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'shctl@builtin'

fff1() {}
$DYNA_UDCS['fff 2'] = (){}
assert complete -d -- 'fff'
assert $COMPREPLY.size() == 2
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'fff1@user-defined'
assert $args[1] == 'fff\ 2@dynamic'

assert complete -d -- '\f\ff\'
assert $COMPREPLY.size() == 2
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args == @('\f\ff\ 2@dynamic' '\f\ff\1@user-defined')

assert complete -d -- 'mkdir'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'mkdir@command'

\9999() {}
assert complete -d -A cmd '999'
assert $COMPREPLY.size() == 1
assert $COMPREPLY[0] == '9999'
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == '9999@user-defined'

###  override existing command
assert complete -d -- 'echo'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'echo@builtin'

assert complete -d -- '$DYNA_UDC["echo"] = (){}; echo'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'echo@builtin'

assert complete -d -- 'echo(){}; echo'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'echo@user-defined'

$DYNA_UDCS["mkdir"] = (){}
assert complete -d -- '; mkdir'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'mkdir@dynamic'

assert complete -d -- 'mkdir(){}; mkdir'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'mkdir@user-defined'

assert complete -d -- '\let(){}; let'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'let@keyword'  # not override keyword

assert complete -d -- '\let(){}; \let'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == '\let@user-defined'

## module/sub command
source $SCRIPT_DIR/../../_module4test/comp.ds as comp234

assert complete -d -- '$comp2'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'comp234@: %mod3'

assert complete -d -- '[$comp234].a'
assert $COMPREPLY.size() == 2
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'add@(value: %mod3): [%mod3] for [%mod3]'
assert $args[1] == 'addAll@(other: [%mod3]): [%mod3] for [%mod3]'

assert complete -d -- 'comp2'
assert $COMPREPLY.size() == 1
$args = @($COMPREPLY)
assert $args.size() == 1
assert $args[0] == 'comp234@module'

assert complete -d -- 'comp234 '
assert $COMPREPLY.size() == 2
$args = @($COMPREPLY)
assert $args.size() == 2
assert $args[0] == 'hogehoge@user-defined'
assert $args[1] == 'zzzaaa123@user-defined'