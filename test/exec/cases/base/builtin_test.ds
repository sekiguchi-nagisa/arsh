# not args
test
assert($? == 1) # always 1

# string op
# check not empty
test hoge
assert($? == 0)

test $'\x00ss'
assert($? == 0)

test ""
assert($? == 1)

test -n hoge
assert($? == 0)

test -n $'\x00hoge'
assert($? == 0)

test -n ''
assert($? == 1)

# check empty
test -z ""
assert($? == 0)

test -z hfoe
assert($? == 1)

test -z $'\x00hfoe'
assert($? == 1)

# string compare
test AA "<" BBB
assert($? == 0)

test bb "<" a
assert($? == 1)

test aaa ">" bbb
assert($? == 1)

test bbb ">" aa
assert($? == 0)

test aa = "aa"
assert($? == 0)

test $'a\x0a' = "a"
assert($? != 0)

test aa == "a"
assert($?  == 1)

test aa '!=' 2
assert($? == 0)


# integer op
# bad integer
test u -ne 34
assert($? == 2)

test $'12\x00' -ne 34
assert($? == 2)

test 12 -ne $'34\x00'
assert($? == 2)

test 23.o -lt 45    # not allow float
assert($? == 2)

test 9223372036854775808 -gt 1  # singed integer 64
assert($? == 2)

test -9223372036854775809 -lt 1
assert($? == 2)
assert "$(test -9223372036854775809 -lt 1 2>&1)" == "ydsh: test: -9223372036854775809: must be integer"

test 1 -eq -9223372036854775809
assert $? == 2
assert "$(test 1 -eq -9223372036854775809 2>&1)" == "ydsh: test: -9223372036854775809: must be integer"

test 23 -eq 23
assert($? == 0)

test 23 -eq -4
assert($? == 1)

test -90 -eq -0090  # treat as decimal
assert $? == 0

test -9 -ne 00
assert($? == 0)

#test -90 -ne -0090
#assert($? == 2)

test -90 -ne -0070
assert($? == 0)

test 0xFF -eq 0Xff
assert $? == 2 # only allow decimal

test 076 -eq 0o0076
assert $? == 2 # only allow decimal

test -9223372036854775808 -lt 23456767
assert($? == 0)

test 9223372036854775807 -gt 1245
assert($? == 0)

test -9223372036854775808 -le -9223372036854775808
assert($? == 0)

test 9223372036854775807 -ge 9223372036854775807
assert($? == 0)


# unary file operator
test -a hogeuhfaer
assert($? == 1)

test -a .
assert($? == 0)

test -a $'/\x00'
assert $? == 2

test -e ..
assert($? == 0)

test -b .
assert($? == 1)

var blk = $(df . | grep /dev)
assert $blk.empty() || (test -b $blk[0]) == (sh -c "test -b ${$blk[0]}")

test -c ..
assert($? == 1)

test -c /dev/null
assert($? == 0)

test -d .
assert($? == 0)

test -f $0
assert($? == 0)

test -g .
assert($? == 1)

test -h /dev/stdin
assert($? == 0)

test -L /dev/stdout
assert($? == 0)

test -k .
assert($? == 1)

let tmp_dir = "$(mktemp -d 2> /dev/null || mktemp -d -t lfreop)"
var pipe = "$tmp_dir/hogefaefpdew_pipe"
mkfifo $pipe
test -p $pipe
assert($? == 0)
rm -f $pipe

test -r /
assert($? == 0)

test -s $0
assert($? == 0)

touch $pipe
test -s $pipe
assert($? == 1)
rm -f $pipe

test -S .
assert($? == 1)

#test -t 0
#assert($? == 0)

touch $pipe
test -u $pipe
assert($? == 1)

test -w $pipe
assert($? == 0)

chmod +x $pipe
test -x $pipe
assert($? == 0)

test -x .
assert($? == 0) # if path is directory always success

test -O $pipe
assert($? == 0)

test -G $pipe
assert($? == 0)

rm -f $pipe

# binary file op
## compare file
test $'.\x00' -ef $'.\x00'
assert $? == 1

test hfuerhfueria -ef fhaerfhure
assert $? == 1

test . -ef .
assert $? == 0

test . -ef fherufhiae
assert $? == 1

test fhurefe -ef .
assert $? == 1

test . -ef /dev/null
assert $? == 1

touch $tmp_dir/FILE
ln -s $tmp_dir/FILE $tmp_dir/link
test $tmp_dir/FILE -ef $tmp_dir/link
assert $? == 0

## compare timestamp
let AAA = "$tmp_dir/AAA"
let BBB = "$tmp_dir/BBB"

echo hello >> $AAA
assert test -f $AAA
touch $AAA
sync

sleep 1

echo world >> $BBB
assert test -f $BBB
touch $BBB
echo hgeoho >> $BBB
sync

### newer timestamp
test hfarieo -nt fhnreufhreui
assert $? == 1

test $AAA -nt fhnreufhreui
assert $? == 1

test fhurehfe -nt $AAA
assert $? == 1

test $AAA -nt $AAA
assert $? == 1 : "$(stat $AAA $BBB)"

test $AAA -nt $BBB
assert $? == 1 : "$(stat $AAA $BBB)"

test $BBB -nt $AAA
assert $? == 0 : "$(stat $AAA $BBB)"

### older timestamp
test hfarieo -ot fhnreufhreui
assert $? == 1

test $AAA -ot fhnreufhreui
assert $? == 1

test fhurehfe -ot $BBB
assert $? == 1

test $BBB -ot $BBB
assert $? == 1 : "$(stat $AAA $BBB)"

test $AAA -ot $BBB
assert $? == 0 : "$(stat $AAA $BBB)"

test $BBB -ot $AAA
assert $? == 1 : "$(stat $AAA $BBB)"

### same timestamp
assert !test $AAA -nt $AAA && !test $AAA -ot $AAA

# invalid option
test 34 -h 34
assert($? == 2)

test 34 = 34 45
assert($? == 2)

test -9 hfueir
assert($? == 2)

test hoge hoge
assert($? == 2)

rm -rf $tmp_dir
true
