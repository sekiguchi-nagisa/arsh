# Array for-in
var a = [1, 2, 3, 5, 7]
var i = 0

for $val in $a {
    assert($val is Int)
	assert($val == $a[$i])
	$i = $i + 1
}
assert($i == 5)


# Map for-in
$i = 0;
var tt : [(String, Int)]
for e in ["a" : 1,
            "b" : 2,
            "c" : 3,
            "d" : 4] {
    assert($e is Tuple<String, Int>)
    $tt.add($e)
    $i = $i + 1
}
assert($i == 4)
assert $tt.size() == 4

$i = 0;
var bb = ["a" : 1,
            "b" : 2,
            "c" : 3,
            "d" : 4]
for e in $bb {
    assert($e is Tuple<String, Int>)
    assert $bb[$e._0] == $e._1
    $i = $i + 1
}
assert($i == 4)

# String for-in
var r = ['あ', '0', '🇯🇵', '漢', '　']
$i = 0
for $a in "あ0🇯🇵漢　" {
    assert($a == $r[$i])
    $i = $i + 1
}
assert($i == $r.size())

var count = 0
for s in "" {
    $count++
}
assert $count == 0