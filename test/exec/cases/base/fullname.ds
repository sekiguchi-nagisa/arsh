
source $SCRIPT_DIR/../../_module4test/fullname.ds as f


say!!() {
    echo not say!! $@
}

## resolve from current module
var ret = $MODULE.fullname('say!!')!
assert $ret == $'%mod2\x00say!!'
assert "$(command -V "$ret")" =~ $/user-defined/
assert "$(eval "$ret" 123)" == 'not say!! 123'

assert $f.say_() == $'%mod3\x00say!!'
assert "$(command -V "${$f.say_()}")" =~ $/user-defined/
assert "$(eval "${$f.say_()}" 000)" == 'say!! 000'

assert $f.say__() == $'%mod3\x00_say!!'
assert "$(command -V ${$f.say__()})" =~ $/user-defined/
assert "$(eval "${$f.say__()}" 000)" == '_say!! 000'

## resolve from foreign module
$ret = $f.fullname('say!!')!
assert $ret == $'%mod3\x00say!!'
assert "$(command -V "$ret")" =~ $/user-defined/
assert "$(eval "$ret" 456)" == 'say!! 456'

$ret = $f.fullname('_say!!')!
assert $ret == $'%mod3\x00_say!!'
assert "$(command -V "$ret")" =~ $/user-defined/
assert "$(eval "$ret" 456)" == '_say!! 456'

assert $f.hey_() == $'%mod4\x00say0' : $f.hey_()
assert "$(command -V "${$f.hey_()}")" =~ $/user-defined/
assert "$(eval "${$f.hey_()}" 9)" == '_say!! 9'

assert $f.hey__() == $'%mod4\x00_say0' : $f.hey__()
assert "$(command -V "${$f.hey__()}")" =~ $/user-defined/
assert "$(eval "${$f.hey_()}" 909)" == '_say!! 909'

$ret = $f.fullname('m')!
assert $ret == $'%mod3\x00m'
assert "$(command -V "$ret")" =~ $/user-defined/
assert "$(eval "$ret" say0 456)" == '_say!! 456'

$ret = $f.fullname('m')!
assert $ret == $'%mod3\x00m'
assert "$(command -V "$ret")" =~ $/user-defined/
assert "$(eval "$ret" _say0 456 2>&1)" == 'ydsh: %mod3: cannot resolve private subcommand: _say0'
assert $? == 1

# resolve builtin, external
$ret = $MODULE.fullname('exit')!
assert $ret == 'exit'
assert "$(command -V "$ret")" =~ $/builtin/

$ret = $f.fullname('exit')!
assert $ret == 'exit'
assert "$(command -V "$ret")" =~ $/builtin/

$ret = $MODULE.fullname('cd')!
assert $ret == 'cd'
assert "$(command -V "$ret")" =~ $/builtin/

$ret = $f.fullname('cd')!
assert $ret == 'cd'
assert "$(command -V "$ret")" =~ $/builtin/

$ret = $MODULE.fullname('env')!
assert $ret == "/usr/bin/env"
assert "$(command -V "$ret")" =~ $/is \/usr\/bin\/env/


# not resolved
assert ! $MODULE.fullname('hogeho')
assert ! $MODULE.fullname('/')
