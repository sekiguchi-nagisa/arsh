
var AAA = 'aaa'

BBB() { echo BBB: $@; }

var aaa = $MODULE.load('$AAA')
assert $aaa is () -> Any!
assert $aaa()! as String == 'aaa'

var bbb = $MODULE.load('BBB a b c')
assert $bbb is () -> Any!
assert $bbb()! as Bool
assert "$($bbb())" == 'BBB: a b c'

var ccc = $MODULE.load('$bbb() as Void')
assert $ccc is () -> Any!
assert ! $ccc()
assert "$($ccc())" == 'BBB: a b c'

# module
source ../../_module4test/module1.ds
assert "$($MODULE.load('say0 1234')())" == "_say!! 1234"

source ../../_module4test/module1.ds as mod
assert $mod.load('$hello')()! as String == 'hello module!!'


# compile error
var ex = 34 as Any
try { $MODULE.load('var _vvv = 34')(); } catch $e { $ex = $e; }
assert $ex is InvalidOperationError
assert $/expected expression/ =~ ($ex as Error).message()

$ex = 234
try { $MODULE.load('echo $WWW')(); } catch $e { $ex = $e; }
assert $ex is InvalidOperationError
assert $/undefined/ =~ ($ex as Error).message()

# runtime error
$ex = 234
try { $mod.load('34 as Any as StrArray')(); } catch $e { $ex = $e; }
assert $ex is TypeCastError

true