# boolean key
var b = [$true : 34, $false : 90]
assert $b[$true] == 34
assert $b[$false] == 90

# Float
var f = [3.14 : 3.14, 0.1e-3: 34 as Float]
assert $f[3.14] == 3.14
assert $f[0.0001] == 34.0
var f2 = [-0.0: "a", 0.0: "b", (0.0/0.0): "c"]
assert $f2.size() == 3
assert $f2[0.0] == "b"
assert $f2[-0.000] == "a"
assert $f2[0.000/-0.000] == "c"

# Signal
var sg = [%'hup' : "hung up", %'kill' : "killed"]
assert $sg[%'SigHuP'] == "hung up"
assert $sg[$SIG.signal("KILL")!] == "killed"

# duplicated key
var dd = ["a" : 23, "a" : 54]
assert $dd.size() == 1
assert $dd['a'] == 54