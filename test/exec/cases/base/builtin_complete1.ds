# complete file name
__file() {
    var type = $# > 1 && $2.startsWith('--') ? $@.pop() : new String?()

    let dir = $@[0]
    for $a in $(ls -A $dir) {
        var name = $dir
        if $name[-1] != '/' {
            $name += '/'
        }
        $name += $a
        if(test -d $name) {
            echo ${$escape2($a)}/
        } else {
            if($@.size() > 1) {
                !(test -f $name && test -x $name) && continue
            }
            echo ${$escape2($a)}
        }
    }
}

file() {
    __file $@ | sort | uniq
}

files() {
    for $p in $@ {
        __file $p
    } | sort | uniq
}

function map(aa: [String], func: (String)->String) : [String] {
    var ret: [String]
    for e in $aa {
        $ret.add($func($e))
    }
    return $ret
}

function assertArray2(expect: [String], actual: Candidates) {
    var aa: [String]
    let s = $actual.size()
    for(var i = 0; $i < $s; $i++) {
        $aa.add($actual[$i])
    }
    $assertArray($expect, $aa)
}

