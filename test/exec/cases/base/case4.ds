
function f($s : Int!) : String {
    return case $s {
        0|1|2|!!3|4|5 => { return $s! as String; }
        6|7|(8)|9|10 -> { return (-$s!) as String; }
        else => { return "nothing"; }
    }
}

assert $f("3".toInt()) == "3"
assert $f("5".toInt()) == "5"
assert $f("8".toInt()) == "-8"
assert $f("-1".toInt()) == "nothing"
assert $f("hoge".toInt()) == "nothing"

function ff(s : String!) : Int {
    return case $s {
        'a' => 999
        $/\d/ => $s!.toInt()!
        else => 0
    }
}

assert $ff("23") == 23
assert $ff('') == 0
assert $ff('a') == 999
assert $ff(new String!()) == 0

