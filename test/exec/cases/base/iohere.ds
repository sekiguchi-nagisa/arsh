
# small string
assert "$(cat <<< hello)" == "hello"
assert "$(cat <<< hello && echo world)" == $'hello\nworld'

# large string
var a = "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
for(var i = 0; $i < 10; $i++) {
    $a = $a + $a
}
assert "$(cat <<< $a && echo !!!)" == $a + $'\n!!!'

# empty command

assert "$(<<< 'hey say!!')" == 'hey say!!'