
## anonymous command

var cc1 = ()
{
  echo $0: $@
}

assert $cc1 is Command
assert "$($cc1.call(@()))" == ":"
assert $? == 0
assert "$($cc1.call(@(aaa bbb ccc)))" == "aaa: bbb ccc"
assert $? == 0

## closure
var cc2 = {
    var v = "echo"
    () 
    {
        eval $v $@
    }
}
assert $cc2 is Command
assert "$($cc2.call(@(hoge hello world)))" == "hello world"