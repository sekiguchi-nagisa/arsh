var count = 1

type Interval {
    var begin: Int
    var end: Int
}

function dist() :Int for Interval {
    return $this.end - $this.begin
}

function merge(other: Interval): Interval for Interval {
    return new Interval(
        $this.begin < $other.begin ? $this.begin : $other.begin,
        $this.end < $other.end ? $other.end : $this.end
    )
}

